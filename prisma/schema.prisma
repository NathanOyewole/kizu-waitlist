generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL") // If using Supabase
}

model Lead {
  id        String   @id @default(uuid())
  email     String   @unique
  createdAt DateTime @default(now())
}
